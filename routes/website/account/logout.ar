import "server" as *
import "database" as *

let logout(req, res) = do
    let redirect = req.query.get("to", "/login")
    if (redirect == "/logout") redirect = "/login"
    term.log(account.deleteToken(req.cookies.get("token", "")))
    res.deleteCookie("token")
    res.redirect(redirect)
app.get("/logout", logout)